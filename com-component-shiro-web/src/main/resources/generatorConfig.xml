<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<classPathEntry location="D:\\repository\\mysql\\mysql-connector-java\\5.1.25\\mysql-connector-java-5.1.25.jar" />
	<context id="mybatis" targetRuntime="MyBatis3Simple"
		defaultModelType="flat">
		<property name="beginningDelimiter" value="`" />
		<property name="endingDelimiter" value="`" />
		<property name="javaFileEncoding" value="UTF-8" />
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="false" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"
			connectionURL="jdbc:mysql://127.0.0.1:3306/shiro_db" userId="root"
			password="123456" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="true" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="com.shiro.module"
			targetProject="src/main/java" >
			<!-- 增加基础bean的继承操作，将通用属性集成到BaseEntity内部(貌似没有用)
			<property name="rootClass" value="com.eif.inspection.base.entity.standardEntity"/>
			-->
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="sqlmapper"
			targetProject="src/main/resources" />
		<javaClientGenerator targetPackage="com.shiro.entity.dao"
			targetProject="src/main/java" type="XMLMAPPER" />

		<!--表名对应实体对象-->
		<table tableName="t_user" domainObjectName="TUser">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>
		<table tableName="t_role" domainObjectName="TRole">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>
		<table tableName="t_user_role" domainObjectName="TUserRole">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>
		<table tableName="t_permission" domainObjectName="TPermission">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>
		<table tableName="t_role_permission" domainObjectName="TRolePermission">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>
		<table tableName="t_user_login_info" domainObjectName="TUserLoginInfo">
			<generatedKey column="id" sqlStatement="JDBC"  identity="true"/>
		</table>

	</context>
</generatorConfiguration>
